  object FDQuery: TFDQuery
    Connection = DmConexao.FDConnection
    SQL.Strings = (
      'SELECT  '
      'TAB_FLUXO.COD_PARCEIRO, '
      'TAB_FLUXO.NUM_CGC_CPF, '
      'TAB_FLUXO.DES_PARCEIRO, '
      'TAB_FLUXO.DTA_EMISSAO, '
      'TAB_FLUXO.COD_BANCO, '
      'TAB_FLUXO.NUM_DOCTO, '
      'TAB_FLUXO.DTA_VENCIMENTO, '
      'TAB_FLUXO.VAL_PARCELA, '
      'TAB_FLUXO.DTA_ENTRADA, '
      'TAB_FLUXO.NUM_PARCELA, '
      'TAB_FLUXO.QTD_PARCELA, '
      'TAB_FLUXO.VAL_TOTAL_NF, '
      'TAB_FLUXO.NUM_BORDERO, '
      'TAB_FLUXO.NUM_PDV, '
      'TAB_FLUXO.COD_LOJA, '
      'TAB_FLUXO.NUM_CUPOM_FISCAL, '
      'TAB_FLUXO.COD_CHAVE, '
      'TAB_FLUXO.DES_BANDEIRA, '
      'TAB_FLUXO.DES_REDE_TEF, '
      'TAB_FLUXO.COD_BIN, '
      'COD_BANDEIRA_TEF, '
      'COD_AUTORIZACAO_TEF, '
      'NUM_BIN_TEF, '
      'NUM_NSU_HOST_TEF, '
      'NUM_NSU_SITEF, '
      'COD_INSTITUICAO_TEF, '
      'COD_ESTABELECIMENTO_TEF, '
      'COD_TRANSACAO_TEF, '
      'COD_ADMINISTRADORA_TEF,  '
      'TAB_FLUXO.DTA_EMISSAO_PGTO'
      'FROM '
      'TAB_FLUXO '
      'LEFT JOIN TAB_PLANO_CONTA PLANCONTA ON '
      '  TAB_FLUXO.COD_CONTA = PLANCONTA.COD_CONTA '
      'LEFT JOIN TAB_ENTIDADE ON'
      ' TAB_FLUXO.COD_ENTIDADE = TAB_ENTIDADE.COD_ENTIDADE'
      'LEFT JOIN TAB_COMPRADOR ON'
      ' TAB_FLUXO.COD_COMPRADOR = TAB_COMPRADOR.COD_COMPRADOR'
      'LEFT JOIN TAB_VENDEDOR ON'
      ' (TAB_FLUXO.COD_USUARIO_VDA = TAB_VENDEDOR.COD_VENDEDOR '
      'AND '
      '  TAB_FLUXO.COD_LOJA = TAB_VENDEDOR.COD_LOJA)'
      'LEFT JOIN TAB_ENTIDADE TAB_ENTIDADE_PDV ON '
      ' TAB_ENTIDADE_PDV.COD_ENTIDADE = TAB_FLUXO.COD_ENTIDADE_PDV'
      'LEFT JOIN TAB_CATEGORIA'
      'ON TAB_CATEGORIA.COD_CATEGORIA = TAB_FLUXO.COD_CATEGORIA '
      'LEFT JOIN TAB_SUBCATEGORIA '
      'ON (TAB_SUBCATEGORIA.COD_CATEGORIA = TAB_FLUXO.COD_CATEGORIA   '
      'AND '
      'TAB_SUBCATEGORIA.COD_SUBCATEGORIA = TAB_FLUXO.COD_SUBCATEGORIA)'
      'LEFT JOIN TAB_CENTRO_RESULT ON '
      
        ' TAB_FLUXO.COD_CENTRO_RESULT = TAB_CENTRO_RESULT.COD_CENTRO_RESU' +
        'LT'
      'LEFT JOIN TAB_FLUXO_BOLETO ON                      '
      ' TAB_FLUXO.COD_CHAVE = TAB_FLUXO_BOLETO.COD_CHAVE   '
      'WHERE '
      'TAB_FLUXO.COD_LOJA = :PAR_LOJA'
      'AND '
      'TAB_FLUXO.TIPO_CONTA =1'
      ' AND TAB_FLUXO.TIPO_PARCEIRO = 4'
      ' AND TAB_FLUXO.VAL_PARCELA = :PAR_VALOR'
      ''
      ' AND TAB_FLUXO.DTA_EMISSAO = :PAR_DATA'
      ' AND TAB_FLUXO.FLG_QUITADO = '#39'N'#39)
    Left = 472
    ParamData = <
      item
        Name = 'PAR_LOJA'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'PAR_VALOR'
        DataType = ftFloat
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'PAR_DATA'
        DataType = ftDate
        ParamType = ptInput
        Value = Null
      end>
